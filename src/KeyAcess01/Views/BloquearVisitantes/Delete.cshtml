@model KeyAcess01.Models.BloquearVisitante

@{
    ViewData["Title"] = "Excluir";
}

<h1>Excluir Bloqueio</h1>

<h3>Tem certeza que deseja excluir o bloqueio deste visitante?</h3>
<div>
    @*<h4>BloquearVisitante</h4>*@
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Apartamento)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Apartamento.NmrApartamento)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Nome)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Nome)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Cpf)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Cpf)
        </dd>
    </dl>

    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Delete" class="btn btn-danger" /> |
        <a class="btn btn-info" asp-action="Index">Voltar</a>
    </form>
</div>
