@model KeyAcess01.Models.Apartamento

@{
    ViewData["Title"] = "Relatorios";
}

<h1>Relatorios</h1>

<div>
    <h4>Apartamento</h4>
    <hr />
    <table class="table">
        <thead>
            <tr>
                <th>
                    Nome
                </th>
                <th>
                    CPF
                </th>
                <th>
                    Telefone
                </th>
                <th>
                    Horário-Entrada
                </th>
                <th>
                    Horário-Saída
                </th>

            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model.Visitantes)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.Nome)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Cpf)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Telefone)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.HrrEnt)
                    <td>
                            @Html.DisplayFor(modelItem => item.HrrSaid)
                    </td>

                </tr>
            }
        </tbody>
    </table>
</div>
<div>
    <a asp-action="Index">Voltar</a>
</div>